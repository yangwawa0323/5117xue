/**
 * Description of Product
 *
 * @author yangwawa-win7
 */
namespace Acme\StoreBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Product {
    
    /**
     * 
     * @MongoDB\ID
     */
    protected $id;
    
    /**
     *
     * @MongoDB\string
     */
    protected $name;
    
    /**
     *
     * @MongoDB\float
     */
    protected $price;
}
